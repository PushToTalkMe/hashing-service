datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../shared/prisma"
}

model AuditLog {
  id             Int      @id @default(autoincrement())
  userId         String?
  action         String
  status         String
  additionalData String?
  createdAt      DateTime @default(now())
}
